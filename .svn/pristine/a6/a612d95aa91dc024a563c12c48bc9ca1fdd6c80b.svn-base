<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>作业发布</title>
		<link rel="stylesheet" href="../../layui/css/layui.css"/>
		<script src="../../jquery-3.2.1.min.js"></script>
		<script src="../../layui/layui.js"></script>
		<!-- 操作列 -->
		<script type="text/html" id="oper-col">
		    <a class="layui-btn layui-btn-normal layui-btn-sm" lay-event="open">查看答题情况</a>
		</script>
		<style>
		    .layui-input {
		        height: 30px;
		    }
		
		    .right {
		        float: right;
				line-height: 30px;
		    }
		
		    body {
		        margin: 10px
		    }
		   
		</style>
	</head>
	<body>
		<!-- 搜索栏 -->
		<div class="layui-row">
			<div class="layui-col-md1">
			    <div class="right">学期：</div>
			</div>
			<div class="layui-col-md2">
			    <select class="layui-input" name="term_id" id="term_id"></select>
			</div>
		    <div class="layui-col-md1">
		        <div class="right">实训名称：</div>
		    </div>
		    <div class="layui-col-md2">
		        <select class="layui-input" name="id" id="id"></select>
		    </div>
			<div class="layui-col-md2">
				&nbsp;&nbsp;
				<button type="button" class="layui-btn layui-btn-sm layui-btn-normal" id="search">查询</button>
				<button type="button" class="layui-btn layui-btn-sm layui-btn-normal" id="open">添加作业</button>
			</div>
		</div>
		<!-- 表格栏 -->
		<div>
			<table id="table" lay-filter="table"></table>
		</div>
	</body>
	<script src="../../config.js"></script>
	<script src="../../tool.js"></script>
	<script src="../js/work.js"></script>
</html>
